[package]
name = "elite-mev-bot-minimal"
version = "0.1.0"
edition = "2021"

[[bin]]
name = "elite_mev_bot_minimal"
path = "src/bin/elite_mev_bot_minimal.rs"

[dependencies]
tokio = { version = "1.21", features = ["rt-multi-thread", "macros"] }
serde = { version = "1.0", features = ["derive"] }
serde_json = "1.0"
anyhow = "1.0"
tracing = "0.1"
futures = "0.3"